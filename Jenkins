pipeline {
    agent any
    tools {
  maven 'Maven 3'
}
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/yaswanth507/javaparser-maven-sample.git'
            }
        }

        stage('Build') {
            steps {
                tool 'Maven 3' // Configure the Maven tool name in Jenkins
                sh 'mvn clean install'
            }
        }
        
      }
      }
